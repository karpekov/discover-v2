{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": false,
			"env": {"PYTHONPATH": "${workspaceFolder}"}
		},
		{
			"name": "Run: training script",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/src-v2/training/train.py",
			"console": "integratedTerminal",
			"justMyCode": false,
			"env": { "PYTHONPATH": "${workspaceFolder}" },
			"args": ["--config", "${workspaceFolder}/src-v2/config/milan_baseline_50.json"]
		}
	]
}
